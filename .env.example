# .env.example
# Copy this file to .env.local and fill in the values


# Resend
RESEND_API_KEY=                           # Required for sending contact form emails
EMAIL_FROM_ADDRESS=                       # The sender address for contact form emails
EMAIL_TO_ADDRESS=                         # Where to receive contact form submissions

# Calendly Integration
CALENDLY_API_KEY=                         # Required if using Calendly API (optional)
NEXT_PUBLIC_CALENDLY_USERNAME=            # Your Calendly username for the embed (required)

# Base URL
NEXT_PUBLIC_BASE_URL=                     # Used for absolute URLs in meta tags and emails
